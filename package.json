{
  "name": "govuk-design-history",
  "version": "2.0.0",
  "description": "A history of the design decisions made for Datahub, including key findings from analysis and research.",
  "keywords": [
    "design-history",
    "government",
    "govuk"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf public",
    "build": "eleventy --quiet",
    "predev": "npm run build",
    "dev": "eleventy --serve --quiet",
    "prestart": "npm run build",
    "start": "http-server",
    "test": "standard"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "govuk-eleventy-plugin": "^2.0.0",
    "govuk-frontend": "^4.0.1",
    "http-server": "^14.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1"
  },
  "devDependencies": {
    "standard": "^17.0.0",
    "webshot-node": "^0.18.2"
  },
  "private": true,
  "engines": {
    "node": "^16.0.0"
  },
  "standard": {
    "ignore": [
      "!.eleventy.js"
    ]
  }
}
